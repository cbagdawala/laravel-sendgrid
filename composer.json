{
  "name": "cbagdawala/laravel-sendgrid",
  "version": "dev-master",
  "description": "SendGrid wrapper for Laravel using sendgrid/sendgrid",
  "type": "library",
  "keywords": [
    "cbagdawala",
    "sendgrid"
  ],
  "homepage": "https://github.com/cbagdawala/laravel-sendgrid",
  "license": "MIT",
  "authors": [
    {
      "name": "Chintan Bagdawala",
      "email": "chintan@bagdawala.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "LaravelSendGrid\\": "src/"
    }
  },
  "require": {
    "sendgrid/sendgrid": "^7.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelSendGrid\\SendGridServiceProvider"
      ],
      "aliases": {
        "SendGrid": "LaravelSendGrid\\Facades\\SendGrid"
      }
    }
  }
}
